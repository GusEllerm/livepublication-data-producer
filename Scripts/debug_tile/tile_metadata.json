{
  "url": "https://sh.dataspace.copernicus.eu/api/v1/process",
  "headers": {
    "content-type": "application/json",
    "accept": "image/tiff"
  },
  "request_type": "POST",
  "data_type": "tiff",
  "payload": {
    "input": {
      "bounds": {
        "properties": {
          "crs": "http://www.opengis.net/def/crs/EPSG/0/4326"
        },
        "bbox": [
          13.822174,
          45.850803,
          14.559345,
          46.291917
        ]
      },
      "data": [
        {
          "type": "sentinel-2-l2a",
          "dataFilter": {
            "timeRange": {
              "from": "2022-06-01T00:00:00Z",
              "to": "2022-06-10T23:59:59Z"
            },
            "mosaickingOrder": "leastCC"
          }
        }
      ]
    },
    "evalscript": "//VERSION=3\nfunction setup() {\n  return {\n    input: [\"B04\", \"B08\"],\n    output: { bands: 2 }\n  };\n}\nfunction evaluatePixel(sample) {\n  return [sample.B04, sample.B08];\n}",
    "output": {
      "responses": [
        {
          "identifier": "default",
          "format": {
            "type": "image/tiff"
          }
        }
      ],
      "width": 100,
      "height": 100
    }
  },
  "save_path": "N/A",
  "folder": "./debug_tile"
}